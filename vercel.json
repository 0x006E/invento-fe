{
  "rewrites": [
    { "source": "/(.*)", "destination": "/" },
    {
      "source": "/api/:path*",
      "destination": "https://invento-server-production.up.railway.app/api/:path*"
    },
    {
      "source": "/api/:path*/",
      "destination": "https://invento-server-production.up.railway.app/api/:path*/"
    }
  ]
}
