{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://invento-server-production-ceee.up.railway.app/api/:path*"
    },
    {
      "source": "/api/:path*/",
      "destination": "https://invento-server-production-ceee.up.railway.app/api/:path*/"
    },
    { "source": "/(.*)", "destination": "/" }
  ]
}
